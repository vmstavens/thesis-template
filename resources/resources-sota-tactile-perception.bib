@article{quasistatic-manipulation-with-compliance-and-sliding,
  author   = {Imin Kao and Mark R. Cutkosky},
  title    = {Quasistatic Manipulation with Compliance and Sliding},
  journal  = {The International Journal of Robotics Research},
  volume   = {11},
